<div>
    <h1 class="title2 titleImp">Ironhack Cinema</h1>
    <h2 class="title2">Click on the movie to check the showtimes!</h2>
</div>

<div class="container">
    <div class="row">
        {{#each movie}}
        <div class="col-4 movies">

            <img src="{{image}}" alt="{{title}}">
            <p>{{title}}</p>
            <a href="/movies/details/{{id}}">See more</a>

        </div>
        {{/each}}
    </div>
</div>